{
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
  "python.analysis.extraPaths": [
    "${workspaceFolder}/src"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.py[cod]": true,
    "**/.pytest_cache": true,
    "**/.benchmarks": true,
    "**/.mypy_cache": true,
    "**/.ruff_cache": true,
    "**/.coverage": true,
    "**/htmlcov": true,
    "**/*.egg-info": true,
    "**/build": true,
    "**/dist": true,
    "**/.venv": true
  },
  "python-envs.defaultEnvManager": "ms-python.python:system",
  "python-envs.pythonProjects": []
}
